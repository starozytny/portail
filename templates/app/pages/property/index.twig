{% extends 'app/layout/layout.twig' %}

{% block title %}Liste des biens {{ parent() }}{% endblock %}

{% block stylesheets %}
    {{ parent() }}
    {% webpack_entry_css 'property' %}
{% endblock %}

{% block main_content %}
    <div class="page-title">
        <div class="page-title-container">
            <h1>Liste des biens</h1>
        </div>
    </div>

    <div id="properties" data-donnees="{{ data | json_encode }}" data-inventories="{{ inventories | json_encode }}"></div>
{% endblock %}

{% block javascripts %}
    {{ parent() }}
    {% webpack_entry_js 'property' %}
{% endblock %}