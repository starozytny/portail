{% extends 'app/pages/edl/template_form.twig' %}

{% block title %}Ajouter un état des lieux {{ parent() }}{% endblock %}

{% block edl_content %}
    <div class="page-title">
        <div class="page-title-container">
            <h1>Ajouter un état des lieux</h1>
        </div>
    </div>

    <div class="main-content">
        <div id="edl-form"
             data-type="create"
             data-users="{{ users | json_encode }}"
             data-current-user="{{ session.user[7] }}"
             data-models="{{ models | json_encode }}"
             data-properties="{{ properties | json_encode }}"
             data-tenants="{{ tenants | json_encode }}"
        ></div>
    </div>

    {{ parent() }}
{% endblock %}